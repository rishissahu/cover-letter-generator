{
    "manifest_version": 3,
    "name": "Cover Letter Generator",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "icons": {
      "128": "logo.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
}
